<div class="right-rail column">
    <section class="grid grid-fixed" masonry item-selector="article" data-ng-init="init()">
            <article class="grid-col--fixed-tiles" style="background-color:white">
                <a href="http://allrecipes.com/HowTo/Making-Apple-Pie-Filling/Detail.aspx" style="position: relative">
                    <div>
                        <img src="http://images.media-allrecipes.com/images/9442.jpg" class="grid-col__rec-image" />
                    </div>
                    <h3 class="grid-col__h3 grid-col__h3--recipe-grid">
                        Making Apple Pie Filling
                    </h3>
                    <div class="counts-div" style="display:block">
                        <span class="article-author">By: Allrecipes Staff</span>
                    </div>
