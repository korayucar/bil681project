    <section class="grid grid-fixed" masonry item-selector="article" data-ng-init="init()">
            <article class="grid-col--fixed-tiles" style="background-color:white">
                <a href="http://allrecipes.com/HowTo/Super-Bowl-Chili-Cook-Off/Detail.aspx" style="position: relative">
                    <div>
                        <img src="http://images.media-allrecipes.com/images/7437.jpg" class="grid-col__rec-image" />
                    </div>
                    <h3 class="grid-col__h3 grid-col__h3--recipe-grid">
                        Super Bowl&#174;: Chili Cook-Off
                    </h3>
                    <div class="counts-div" style="display:block">
                        <span class="article-author">By: Cory Vicens</span>
                    </div>
                </a>
            </article>
            <article class="grid-col--fixed-tiles" style="background-color:white">
