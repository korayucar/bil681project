    <ar-save-item class="favorite" data-id="31773" data-type="'Recipe'" data-name="&quot;West Texas-Style Buffalo Chili&quot;" data-imageurl="'http://images.media-allrecipes.com/userphotos/250x250/1085060.jpg'"></ar-save-item>





